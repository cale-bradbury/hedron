{
  "name": "Hedron",
  "version": "0.4.1",
  "repository": {},
  "license": "AGPL-3.0+",
  "scripts": {
    "lint": "eslint bin build config server src tests",
    "lint:css": "stylelint \"src/components/**/*.js\"",
    "lint:fix": "yarn lint -- --fix",
    "test": "yarn lint && cross-env NODE_ENV=test tape-watch --once -r babel-register src/**/*.spec.js mockTests/**/*.spec.js -p tap-diff",
    "test:dev": "cross-env NODE_ENV=test tape-watch -r babel-register src/**/*.spec.js mockTests/**/*.spec.js -p tap-diff",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dev": "yarn dist -- --dir -c.compression=store -c.mac.identity=null && open dist/mac/hedron.app/ --args --distDev",
    "dist:example-projects": "yarn --cwd ./example-projects && zip dist/example-projects.zip ./example-projects -r",
    "start": "yarn electron-webpack dev"
  },
  "electronWebpack": {
    "renderer": {
      "webpackConfig": "config/webpack.custom.config.js"
    }
  },
  "build": {
    "linux": {
      "target": [
        "AppImage"
      ]
    }
  },
  "glslify": {
    "transform": [
      "glslify-import"
    ]
  },
  "dependencies": {
    "@tweenjs/tween.js": "^17.2.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.22.0",
    "create-react-class": "^15.5.2",
    "electron-debug": "^1.4.0",
    "electron-log": "^2.2.14",
    "err-code": "^1.1.2",
    "eventemitter3": "^3.0.1",
    "glslify-import": "^3.1.0",
    "history": "^4.6.1",
    "jsonfile": "^2.4.0",
    "lodash": "^4.17.4",
    "performance-now": "^2.1.0",
    "proxyquire": "^1.7.11",
    "react": "^16.7.0",
    "react-addons-perf": "^15.4.2",
    "react-create-class": "^1.0.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "next",
    "react-input-autosize": "^1.1.3",
    "react-redux": "^5.0.2",
    "react-router-dom": "next",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-select": "^1.0.0-rc.3",
    "recompose": "^0.26.0",
    "redux": "^3.6.0",
    "redux-action-listeners": "^1.0.2",
    "redux-batched-subscribe": "^0.1.6",
    "redux-debounce": "^1.0.1",
    "redux-form": "^7.2.3",
    "redux-ignore": "^1.2.4",
    "redux-saga": "^0.16.0",
    "stats.js": "^0.17.0",
    "styled-components": "^4.1.3",
    "tap-diff": "^0.1.1",
    "tap-tempo": "^0.1.1",
    "three": "^0.100.0",
    "three-effectcomposer": "^0.0.1",
    "tinycolor2": "^1.4.1",
    "try-require": "^1.2.1",
    "uid": "^0.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.22.0",
    "clean-webpack-plugin": "^0.1.16",
    "concurrently": "^3.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.7",
    "deep-freeze": "^0.0.1",
    "electron": "^1.8.2-beta.2",
    "electron-builder": "^19.43.0",
    "electron-devtools-installer": "^2.2.1",
    "electron-webpack": "^1.11.0",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.0.1",
    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "minimist": "^1.2.0",
    "redux-debounced": "^0.4.0",
    "redux-devtools-extension": "^2.13.2",
    "sinon": "^2.1.0",
    "style-loader": "^0.16.1",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-processor-styled-components": "^0.1.0",
    "svg-inline-loader": "^0.7.1",
    "tap-colorize": "^1.2.0",
    "tape": "^4.6.3",
    "tape-watch": "^2.3.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.4.1"
  }
}
